             xmlns:local="clr-namespace:NegosudClient"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/Components/ComboStyle.xaml"/>
